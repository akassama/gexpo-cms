<script>
    //function to get base url
    function getBaseUrl() {
        return window.location.href.match(/^.*\//);
    }

    //remove Home if it exists in url
    function removeHome(url) {
        if (url.indexOf("/Home") >= 0) {
            url = url.replace('/Home', '');
        }
        return url;
    }
</script>
