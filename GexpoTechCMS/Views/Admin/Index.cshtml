@using NgoExpoApp.App_Code
@{
    ViewData["Title"] = "Admin Page";

    //used to enable admin sidebar
    ViewBag.LoggedInPage = "true";

    //used for setting breadcrumb
    ViewData["BreadCrumb-0"] = null;
    ViewData["BreadCrumb-0-Text"] = null;
    ViewData["BreadCrumb-1"] = null;
    ViewData["BreadCrumb-1-Text"] = null;

    //used for setting active left sidebar
    ViewBag.ActiveTab = "Index";
}

<div class="container-fluid p-0">

    <!-- ADMIN SCRIPTS -->
    <partial name="~/Views/Shared/AccountLayout/_AdminScript.cshtml" />

    <!-- SEARCH RESULT DIV -->
    <partial name="~/Views/Shared/AccountLayout/_AdminSearchList.cshtml" />

    <div class="row mb-2 mb-xl-3">
        <div class="col-auto d-none d-sm-block">
            <h3><strong>Analytics</strong> Dashboard</h3>
        </div>

        <!-- BREADCRUMB RECENT VISITS -->
        <partial name="~/Views/Shared/AccountLayout/_Breadcrumbs.cshtml" />
    </div>

    <!-- Action messages partial -->
    <partial name="~/Views/Shared/_ProcessMessage.cshtml" />

    <div class="row">
        <!-- STAT FIGURES -->
        <partial name="~/Views/Shared/AccountLayout/_DashboardStatFigures.cshtml" />

        <!-- RECENT VISITS -->
        <partial name="~/Views/Shared/AccountLayout/_DashboardRecentVists.cshtml" />
    </div>

    <div class="row">
        <!-- BROWSER USAGE -->
        <partial name="~/Views/Shared/AccountLayout/_BrowserUsage.cshtml" />

        <!-- REALTIME VISITS -->
        <partial name="~/Views/Shared/AccountLayout/_RealTimeVisits.cshtml" />

        <!-- CALENDER -->
        <partial name="~/Views/Shared/AccountLayout/_Calendar.cshtml" />
    </div>

    <div class="row">
        <!-- RECENT POSTS -->
        <partial name="~/Views/Shared/AccountLayout/_RecentPosts.cshtml" />

        <!-- MONTHLY VISITS -->
        <partial name="~/Views/Shared/AccountLayout/_MonthlyVisits.cshtml" />
    </div>

</div>


<!-- ADMIN MODALS -->
<partial name="~/Views/Shared/AccountLayout/_AdminModals.cshtml" />


<!-- DASHBOARD SCRIPTS -->
<partial name="~/Views/Shared/AccountLayout/_StatScripts.cshtml" />